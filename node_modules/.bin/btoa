else 
  NODE_PATH="/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/btoa/1.2.1/node_modules/btoa/bin/btoa.js/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/btoa/1.2.1/node_modules/btoa/bin/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/btoa/1.2.1/node_modules/btoa/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/btoa/1.2.1/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/btoa/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/node_modules:/mnt/c/Users/verucchio/Desktop/node_modules:/mnt/c/Users/verucchio/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/btoa/bin/btoa.js/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/btoa/bin/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/btoa/node_modules" node  "$basedir/../btoa/bin/btoa.js" "$@"
