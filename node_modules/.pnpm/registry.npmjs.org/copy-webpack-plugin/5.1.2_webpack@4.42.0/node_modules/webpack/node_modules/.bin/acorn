#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  NODE_PATH="/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules/acorn/bin/acorn/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules/acorn/bin/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules/acorn/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/acorn/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/node_modules:/mnt/c/Users/verucchio/Desktop/node_modules:/mnt/c/Users/verucchio/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" "$basedir/node"  "$basedir/../../../../../../acorn/6.4.2/node_modules/acorn/bin/acorn" "$@"
  ret=$?
else 
  NODE_PATH="/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules/acorn/bin/acorn/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules/acorn/bin/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules/acorn/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/acorn/6.4.2/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/acorn/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/registry.npmjs.org/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules/.pnpm/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/complete/node_modules:/mnt/c/Users/verucchio/Desktop/vaadin-ui-db/node_modules:/mnt/c/Users/verucchio/Desktop/node_modules:/mnt/c/Users/verucchio/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules" node  "$basedir/../../../../../../acorn/6.4.2/node_modules/acorn/bin/acorn" "$@"
  ret=$?
fi
exit $ret
